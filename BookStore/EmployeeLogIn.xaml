<Window x:Class="GUI.EmployeeLogIn"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI"
        mc:Ignorable="d"
        Title="Employee Log In" Height="150" Width="280">
    <Grid>
        <Label Content="Enter Name:" HorizontalAlignment="Left" Margin="48,23,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Margin="127,28,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Enter Password:" HorizontalAlignment="Left" Margin="31,53,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Margin="127,57,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btnLogIn" Content="Log In" HorizontalAlignment="Left" Margin="207,90,0,0" VerticalAlignment="Top" Click="btnLogIn_Click"/>

    </Grid>
</Window>
